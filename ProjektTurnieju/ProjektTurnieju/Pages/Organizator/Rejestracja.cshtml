@page
@model ProjektTurnieju.RejestracjaModel
@{
}
<h1>Rejestracja nowego zawodnika</h1>
<br />
<div class="container">
	<form class="form-horizontal" method="post">
		<div class="form-group">
			<label asp-for="newZawodnik.Imie" class="control-label col-sm2"></label>
			<div class="col-sm-6">
				<input asp-for="newZawodnik.Imie" class="form-control">
			</div>
		</div>
		<br />
		<div class="form-group">
			<label asp-for="newZawodnik.Nazwisko" class="control-label col-sm2"></label>
			<div class="col-sm-6">
				<input asp-for="newZawodnik.Nazwisko" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label asp-for="newZawodnik.Nick" class="control-label col-sm2"></label>
			<div class="col-sm-6">
				<input asp-for="newZawodnik.Nick" class="form-control">
			</div>
		</div>
		<br />
		<div class="form-group">
			<div class="col-sm-offset-2 col-sm-10">
				<button class="btn btn btn-primary">Zatwierdź</button>
			</div>
		</div>
	</form>
</div>
